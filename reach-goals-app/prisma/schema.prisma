generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("REACHGOALS_URL")
  directUrl = env("REACHGOALS_URL_NON_POOLING")
}

model Meta {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  create      DateTime @default(now())
}

model Assignment {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  duration    Int?
  create      DateTime @default(now())
}
